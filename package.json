    "@types/uuid": "^10.0.0",
    "axios": "^1.13.3",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^3.0.5",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.2",
