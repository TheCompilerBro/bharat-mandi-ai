{
  "name": "multilingual-mandi-challenge",
  "version": "1.0.0",
  "description": "AI-powered multilingual trading platform for Indian markets",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc && vite build",
    "build:backend": "tsc",
    "build:frontend": "vite build",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "dev:demo": "ts-node-dev --respawn --transpile-only src/demo-server.ts",
    "dev:frontend": "vite",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "mandi",
    "multilingual",
    "trading",
    "ai",
    "translation"
  ],
  "author": "MandiChallenge Team",
  "license": "MIT",
